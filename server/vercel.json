{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**", "dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "ACCESS_TOKEN_SECRET": "@access_token_secret",
    "REFRESH_TOKEN_SECRET": "@refresh_token_secret"
  }
}
